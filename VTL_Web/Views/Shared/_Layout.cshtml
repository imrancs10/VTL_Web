    <script type="text/javascript">
        // jQuery('.fontSizeEvent').trigger('fontSelected');

        function set_font_size(fontEvent) {
            event.stopPropagation();
            event.preventDefault();
            var fs = jQuery("body").css("font-size");
            var fontSize = fs;
            if (fontEvent == "increase") {
                if (parseInt(fontSize) < 18) {
                    fontSize = parseInt(fontSize) + 2;
                }
            } else if (fontEvent == "decrease") {
                if (parseInt(fontSize) > 10) {
                    fontSize = parseInt(fontSize) - 2;
                }
            } else {
                fontSize = 14;
            }

            $(this).addClass('link-selected').parent().siblings().find('a').removeClass('link-selected')
            jQuery("body").attr('style', 'font-size: ' + fontSize + 'px !important');
        }
    </script>
