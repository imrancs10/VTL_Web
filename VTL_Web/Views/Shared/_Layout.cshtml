    <script type="text/javascript" language="javascript">
        function renderTime() {
            var currentdate = new Date();
            var currentTime = new Date();
            var diem = "AM";
            var h = currentTime.getHours();
            var m = currentTime.getMinutes();
            var s = currentTime.getSeconds();
            var TodayDate = currentdate.getDay() + "/" + currentdate.getMonth() + "/" + currentdate.getFullYear();
            setTimeout('renderTime()', 1000);
            if (h == 0) {
                h = 12;
            } else if (h > 12) {
                h = h - 12;
                diem = "PM";
            }
            if (h < 10) {
                h = "0" + h;
            }
            if (m < 10) {
                m = "0" + m;
            }
            if (s < 10) {
                s = "0" + s;
            }
            var myClock = document.getElementById('clockDisplay');

            var d = new Date();
            var month = d.getMonth() + 1;
            var day = d.getDate();
            var dayOfWeek = d.getDay();
            var output = (day < 10 ? '0' : '') + day + "-" + (month < 10 ? '0' : '') + month + '-' + d.getFullYear();

            myClock.innerHTML = getDayOfWeek(d) + ":" + output + ' ' + " " + h + ":" + m + ":" + s + " " + diem;
        }
        renderTime();
        function getDayOfWeek(date) {
            var day = date.getDay();
            if (day == 0)
                return "Sunday";
            else if (day == 1)
                return "Monday";
            else if (day == 2)
                return "Tuesday";
            else if (day == 3)
                return "Wednesday";
            else if (day == 4)
                return "Thursday";
            else if (day == 5)
                return "Friday";
            else if (day == 6)
                return "Saturday";
        }
    </script>
